/* pages/viewbox/viewbox.wxss */
.page-section-ctn {
  text-align: center;
}

 .swiper-image {
      height: 100%;
      width: 100%;
    
    }

.abstract{
  color:#616161;
}
.upname{
  color: #616161;
  width:auto;
  overflow: hidden;
  height:1.7em;
}

.title{
  color: #616161;
  width:6.0em;
  overflow: hidden;
  height:1.7em;
}
.image {
  margin-top: 30rpx;
  width: 580rpx;
  height: 208rpx;
}

.page{ 
 display: flex; 
 flex-direction: column; 
 height: 100%; 
} 
.navbar{ 
 flex: none; 
 display: flex; 
 background: #fff; 
} 
.navbar .item{ 
 position: relative; 
 flex: auto; 
 text-align: center; 
 line-height: 80rpx; 
} 
.navbar .item.active{ 
 color: #17A8E2; 
} 
.navbar .item.active:after{ 
 content: ""; 
 display: block; 
 position: absolute; 
 bottom: 0; 
 left: 0; 
 right: 0; 
 height: 4rpx; 
 background: #17A8E2; 
} 
.card {
    border: 2px solid #ffffff;
    border-radius: 5px;
    background-color: #ffffff;
    box-shadow: 4px 1px 1px #cccccc;
    margin: 8px;
    position: relative;
}

.image {
    width: 100%;
    height: 170px;
    background-color: #eeeeee;
}

.image2 {
    width: 100%;
    height: 150px;
    background-color: #eeeeee;
}
.keycard{
  width: auto;
  height:1.7em;
  color:#fff;
  line-height:1.7em;
  padding-left:10rpx;
  padding-right:10rpx; 
  border-radius:10rpx;
  background:rgb(199, 199, 199);
  text-align: center;
}
.info{
  display:flex;
  flex-direction: row;
  justify-content: space-between;
}
